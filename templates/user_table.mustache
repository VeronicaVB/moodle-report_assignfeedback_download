<table class="flexible table assignfeedback_download" data-selected-assign="{{assignids}}">
    <thead>
        <tr>
            <th>{{#str}}select, report_assignfeedback_download{{/str}} <br>
                <input type="checkbox" name="select_all" id="selectall">
            </th>
            <th colspan="4">{{#str}}nameheader, report_assignfeedback_download{{/str}}</th>

        </tr>
    </thead>
    <tbody id="iassignfeedbacktb">
        {{#users}}
        <tr>
            <td><input type="checkbox" id="user_{{id}}"></td>
            <td> {{{namelastname}}}</td>
            <td>
                <table id='innertable_{{id}}' class='table-borderless innertable' data-user-grade-id="{{{itemids}}}">
                    <thead>
                        <tr>
                            <th>{{#firstuser}} {{#str}}assessmentheader, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}fileattach, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}feedbackfiles, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th title = "{{#str}}commenthelp, report_assignfeedback_download{{/str}}">
                                {{#firstuser}} {{#str}}feedbackcomments, report_assignfeedback_download{{/str}} 
                                <i class="fa fa-question-circle qcomment" aria-hidden="true"></i>
                                {{/firstuser}}
                            </th>
                            <th>{{#firstuser}} {{#str}}annotatepdf, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}rubric, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}finalgrade, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#assessments}}
                        <tr class ='innertabletr'>
                            <td style="width:14%;">
                                <a href="{{assignmentnameurl}}" target="_blank">{{assignmentname}}</a>
                            </td>
                            <td style="width:14%;">
                                {{#submtree}}
                                {{#tree}}
                                {{{submissionfiletree}}}
                                {{/tree}}
                                {{/submtree}}
                            </td>
                            <td style="width:14%;">
                                {{#feedbackfiletree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/feedbackfiletree}}
                            </td>
                            <td style="width:14%;">
                                {{{feedbackcommentxt}}}
                                 {{#feedbackcomment}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/feedbackcomment}}

                            </td>
                            <td style="width:14%;">
                                {{#annottedpdftree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/annottedpdftree}}
                            </td>
                            <td style="width:14%;"  class ="rubric-cell">
                            {{#frubric}}
                                <div class = 'frubric-container' data-frubric-filling = "{{rubric}}" data-rubric-file-name = "{{rubricfilename}}"> 
                                     {{{frubrictree}}}
                                </div>
                             {{/frubric}}
                            </td>
                            <td style="width:14%;">
                                <strong> {{finalgrade}} </strong>
                            </td>
                        </tr>
                        {{/assessments}}
                    </tbody>
                </table>

            </td>
        </tr>
        {{/users}}
    </tbody>
</table>