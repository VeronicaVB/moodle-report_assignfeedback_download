<table class="flexible table assignfeedback_download" data-selected-assign="{{assignids}}">
    <thead>
        <tr>
            <th>{{#str}}select, report_assignfeedback_download{{/str}} <br>
                <input type="checkbox" name="select_all" id="selectall">
            </th>
            <th colspan="4">{{#str}}nameheader, report_assignfeedback_download{{/str}}</th>

        </tr>
    </thead>
    <tbody id="iassignfeedbacktb">
        {{#users}}
        <tr>
            <td><input type="checkbox" id="user_{{id}}"></td>
            <td> {{{namelastname}}}</td>
            <td>
                <table id='innertable_{{id}}' class='table-borderless innertable' data-user-grade-id="{{{itemids}}}">
                    <thead>
                        <tr>
                            <th>{{#firstuser}} {{#str}}assessmentheader, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}fileattach, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}annotatepdf, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}feedbackfiles, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#assessments}}
                        <tr>
                            <td style="width:25%;">
                                <a href="{{assignmentnameurl}}" target="_blank">{{assignmentname}}</a>

                            </td>
                            <td style="width:25%;">
                                {{#submtree}}
                                {{#tree}}
                                {{{submissionfiletree}}}
                                {{/tree}}
                                {{/submtree}}
                            </td>
                            <td style="width:25%;">
                                {{#annottedpdftree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/annottedpdftree}}
                            </td>
                            <td style="width:25%;">
                                {{#feedbackfiletree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/feedbackfiletree}}
                            </td>
                        </tr>
                        {{/assessments}}
                    </tbody>
                </table>

            </td>



        </tr>

        {{/users}}
    </tbody>
</table>