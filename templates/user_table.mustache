<table class="flexible table assignfeedback_download" data-selected-assign="{{assignids}}" data-selected-cmid="{{cmids}}">
    <thead>
        <tr>
            <th>{{#str}}select, report_assignfeedback_download{{/str}} <br>
                <input type="checkbox" name="select_all" id="selectall">
            </th>
            <th colspan="4">{{#str}}nameheader, report_assignfeedback_download{{/str}}</th>

        </tr>
    </thead>
    <tbody id="iassignfeedbacktb">
        {{#users}}
        <tr>
            <td><input type="checkbox" id="user_{{id}}"></td>
            <td> {{{namelastname}}}</td>
            <td>
                <table id='innertable_{{id}}' class='table-borderless innertable' data-user-grade-id="{{{itemids}}}">
                    <thead>
                        <tr>
                            <th>{{#firstuser}} {{#str}}assessmentheader, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}fileattach, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}onlinesubmission, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>{{#firstuser}} {{#str}}feedbackfiles, report_assignfeedback_download{{/str}} {{/firstuser}}</th>

                            <th title = "{{#str}}commenthelp, report_assignfeedback_download{{/str}}">
                                {{#firstuser}} {{#str}}feedbackcomments, report_assignfeedback_download{{/str}}{{/firstuser}}
                            </th>
                            <th>{{#firstuser}} {{#str}}annotatepdf, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                            <th>
                                {{#firstuser}}
                                 <span>
                                    {{#str}}rubric, report_assignfeedback_download {{/str}}
                                    <i class="fa fa-question-circle" aria-hidden="true" data-toggle="tooltip" data-placement="top"  data-html="true" title="{{#str}}rubric_help, report_assignfeedback_download {{/str}}"></i>
                                 </span>
                                {{/firstuser}}
                            </th>
                            <th>{{#firstuser}} {{#str}}finalgrade, report_assignfeedback_download{{/str}} {{/firstuser}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#assessments}}
                        <tr class ='innertabletr' data-cmid={{cmid}}>
                            <td style="width:14%;">
                                <a href="{{assignmentnameurl}}" target="_blank">{{assignmentname}}</a>
                            </td>
                            <td style="width:14%;">
                                {{#submtree}}
                                {{#tree}}
                                {{{submissionfiletree}}}
                                {{/tree}}
                                {{/submtree}}
                            </td>
                            <td>
                                {{#onlinetxtsubmissionview}}
                                    <a href="{{{onlinetxturl}}}" target ="_blank">
                                    <i class="icon fa fa-search-plus fa-fw " title="View online text submission" aria-label="View online text submission"></i>
                                </a>
                                {{/onlinetxtsubmissionview}}
                                {{{onlinetextsubmission}}}
                            </td>
                            <td style="width:14%;">
                                {{#feedbackfiletree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/feedbackfiletree}}
                            </td>
                            <td style="width:14%;">
                                {{#feedbackview}}
                                <a href="{{{url}}}" target ="_blank">
                                    <i class="icon fa fa-search-plus fa-fw " title="View feedback" aria-label="View feedback"></i>
                                </a>
                                {{/feedbackview}}
                                {{{feedbackcommentxt}}}
                            </td>
                            <td style="width:14%;">
                                {{#annottedpdftree}}
                                {{#tree}}
                                {{{feedbackfiletree}}}
                                {{/tree}}
                                {{/annottedpdftree}}
                            </td>
                            <td style="width:14%;"  class ="rubric-cell">
                            {{#frubric}}
                                <div class = 'frubric-container' data-frubric-params = "{{rubricparams}}" data-frubric-filling = "{{rubric}}" data-rubric-file-name = "{{rubricfilename}}">
                                     {{{frubrictree}}}
                                </div>
                             {{/frubric}}
                            </td>
                            <td style="width:14%;">
                                {{#notgraded}}
                                    Not Graded
                                {{/notgraded}}
                                {{^notgraded}}
                                    <strong> {{finalgrade}} </strong>
                                {{/notgraded}}
                            </td>
                        </tr>
                        {{/assessments}}
                    </tbody>
                </table>

            </td>
        </tr>
        {{/users}}
    </tbody>
</table>